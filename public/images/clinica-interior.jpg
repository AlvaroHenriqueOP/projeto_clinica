<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <!-- Fundo Branco -->
  <rect width="800" height="600" fill="#f8f9fa" />

  <!-- Linhas do chão -->
  <rect x="0" y="350" width="800" height="250" fill="#e9ecef" />
  <line x1="0" y1="350" x2="800" y2="350" stroke="#dee2e6" stroke-width="2" />

  <!-- Teto -->
  <rect x="0" y="0" width="800" height="80" fill="#e9ecef" />

  <!-- Paredes Laterais -->
  <rect x="0" y="80" width="50" height="270" fill="#ced4da" />
  <rect x="750" y="80" width="50" height="270" fill="#ced4da" />

  <!-- Janela -->
  <rect x="550" y="100" width="150" height="200" fill="#adb5bd" />
  <rect x="560" y="110" width="130" height="180" fill="#4dabf7" />
  <line x1="625" y1="110" x2="625" y2="290" stroke="#ffffff" stroke-width="2" />
  <line x1="560" y1="200" x2="690" y2="200" stroke="#ffffff" stroke-width="2" />

  <!-- Cadeira Odontológica -->
  <rect x="200" y="300" width="250" height="40" rx="10" fill="#495057" />
  <rect x="220" y="200" width="210" height="100" rx="5" fill="#495057" />
  <rect x="300" y="150" width="50" height="50" rx="25" fill="#495057" />

  <!-- Equipamento Odontológico -->
  <rect x="450" y="200" width="60" height="150" fill="#6c757d" />
  <circle cx="480" cy="180" r="20" fill="#6c757d" />
  <line x1="480" y1="180" x2="450" y2="200" stroke="#6c757d" stroke-width="5" />
  <line x1="480" y1="180" x2="510" y2="200" stroke="#6c757d" stroke-width="5" />

  <!-- Mesa e Equipamentos -->
  <rect x="100" y="400" width="120" height="60" fill="#868e96" />
  <rect x="120" y="460" width="80" height="5" fill="#495057" />
  <rect x="120" y="390" width="80" height="10" fill="#343a40" />

  <!-- Luminária -->
  <line x1="325" y1="0" x2="325" y2="100" stroke="#adb5bd" stroke-width="2" />
  <ellipse cx="325" cy="120" rx="60" ry="20" fill="#f8f9fa" stroke="#adb5bd" stroke-width="2" />

  <!-- Decorações modernas da clínica -->
  <rect x="50" y="100" width="80" height="120" fill="#4dabf7" stroke="#343a40" stroke-width="1" />
  <rect x="650" y="400" width="80" height="120" fill="#4dabf7" stroke="#343a40" stroke-width="1" />

  <!-- Texto -->
  <text x="400" y="550" font-family="Arial" font-size="20" fill="#343a40" text-anchor="middle">Interior da Clínica Odontológica</text>
</svg> 